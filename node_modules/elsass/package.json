{
  "name": "elsass",
  "version": "0.1.9-beta.8",
  "description": "A high-quality UI component library based on Vue.js",
  "main": "./src/main.js",
  "keywords": [
    "rcc elsass ui"
  ],
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve ./src/main.js",
    "serve:example": "vue-cli-service serve --mode example ./example/pc/main.js",
    "build": "vue-cli-service build --name elsass ./src/main.js",
    "build:example": "vue-cli-service build --dest production_dist --target app --name elsass ./example/pc/main.js",
    "build:lib": "vue-cli-service build --dest lib --target lib --name elsass ./src/lib.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "eslint-plugin-promise": "^4.2.1",
    "iview": "^3.4.2",
    "quill-image-drop-module": "1.0.3",
    "quill-image-extend-module": "1.1.2",
    "quill-image-resize-module": "3.0.0",
    "store": "^2.0.12",
    "vue": "^2.6.10",
    "vue-quill-editor": "3.0.6",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.0",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "compression-webpack-plugin": "^3.0.0",
    "terser-webpack-plugin": "^2.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-components": "^1.0.4",
    "babel-plugin-import": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-anchor": "^5.2.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-decorate": "^1.2.2",
    "markdown-it-task-checkbox": "^1.0.6",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "transliteration": "^2.1.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
